
public class Main
{
	public static void mymethod() {
	    System.out.println("sanmathi");
	}
	
	public static void main(String[] args) {
	    mymethod();
	    
	    
	}
}

public class Main
{
	public static void mymethod(String name) {
	    System.out.println(name);
	}
	
	public static void main(String[] args) {
	    mymethod("sanmathi");
	    
	    
	}
}

public class Main
{
	public static void mymethod(String fname,String lname,int age,int spr) {
	    System.out.println(fname+" "+lname+" "+age+" "+spr);
	}
	
	public static void main(String[] args) {
	    mymethod("sanmathi","chelladurai",20,9476);
	    
	    
	}
}
