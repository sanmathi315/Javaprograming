public class Main
{
	public static void mymethod() {
	    System.out.println("sanmathi");
	}
	
	public static void main(String[] args) {
	    mymethod();
	    
	    
	}
}

public class Main
{
	public static void mymethod(String name) {
	    System.out.println(name);
	}
	
	public static void main(String[] args) {
	    mymethod("sanmathi");
	    
	    
	}
}
