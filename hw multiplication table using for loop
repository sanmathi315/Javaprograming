import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		int num;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter number:");
		int number=sc.nextInt();
		for(int k=1;k<=10;k++){
		    System.out.println(number +" * "+k +" = " +(number+k));
		}
		
		
	}
}
