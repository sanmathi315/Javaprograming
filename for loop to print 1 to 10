public class Main
{
	public static void main(String[] args) {
	    for (int count=1;count <=10; count++)
	    {
	        
	    	System.out.println(count);   
	    }
		
	}
}

public class Main
{
	public static void main(String[] args) {
	    for (int count=10 ;count >=1; count--)
	    {
	        
	    	System.out.println(count);
	    
	    }
	    System.out.println("LIFTOFF");
		
	}
}
