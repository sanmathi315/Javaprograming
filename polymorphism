
(* OVERIDING *)
class Vehicle {
    void drive() {
        System.out.println("drive");
    }
}

class Bike extends Vehicle {
    @Override
    void drive() {
        System.out.println("driving bike");
    }
}

class Car extends Vehicle {
    @Override
    void drive() {
        System.out.println("driving car");
    }
}

public class Main {
    public static void main(String[] args) {
        Vehicle v = new Bike();
        v.drive();

        Vehicle a = new Car();
        a.drive();

        Vehicle b = new Vehicle();
        b.drive();
    }
}
