public class Main
{
	public static void main(String[] args) {
	    String[]array={"san","amsa","susmi"};
	    
		System.out.println (array[2]);
	}
}
MODIFY ARRAY
public class Main
{
	public static void main(String[] args) {
	    String[]array={"san","amsa","susmi"};
	    array[1]="abi";
	    System.out.println (array[1]);
	}
}
